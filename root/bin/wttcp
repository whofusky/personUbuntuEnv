versionNo="software version number: v0.0.0.1"

tDir="/home/fusky/DoChat/WeChat Files/tt"

if [ ! -d "${tDir}" ];then
    echo -e "\n\tERROR: dir [ ${tDir} ] not exist!\n"
    exit 1
fi

if [ $# -ne 1 ];then
    echo -e "\n\tERROR: input like: $0 <file>!\n"
    exit 2
fi
inPar="$1"
echo "cp ${inPar} ${tDir}"
cp ${inPar} "${tDir}"

exit 0

